<h1> All The Delicious Cocktails </h1>
<%= cl_image_tag @cocktail.photo, height: 50 %>  
<%@cocktails.each do |cocktail| %>
<li><h1><%= cocktail.name %><h1></li>
<li><%= link_to 'Show', cocktail %></li>      
<li><%= link_to 'Edit' , edit_cocktail_path(cocktail)%></li>
<li><%= link_to 'Destroy', cocktail_path(cocktail), method: :delete, :data => {:confirm =>"Are you sure?!"} %><li>
  <% end %>
  <%= cl_image_tag @cocktail.photo, height: 50 %>